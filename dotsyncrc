# Location of your dotfiles in $HOME
# DOTFILES=.dotfiles

# dotfiles to symlink
#
# srcfile:dstfile
# dstfile will be '.$srcfile' by default (with path removed)
#
[files]
# Uncomment examples below
dotsyncrc
bin:bin
ctags/ctags
curl/curlrc
git/gitconfig
git/gitignore
git/gitattributes
git/tigrc
gnupg
grc
input/editrc
input/inputrc
postgresql/psqlrc
python/pythonrc
python/pylintrc
screen/screenrc
shell/hushlogin
shell/urlview
ssh
task/task
task/taskrc
tmux/tmux.conf
tmux/tmuxinator
vim/vim
vim/vimrc
wget/wgetrc
zsh/zshrc
[endfiles]

# Hosts to sync
## hostname                     type=masterhost, ANY or NONE    files=dotfile:tolink,another:tolink
[hosts]
laptop                           git=ANY
[endhosts]

